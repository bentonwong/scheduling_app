<h1>Shifts</h1>

<div>
  <table>
    <tr>
      <th>ID</th>
    </tr>
    <% @shifts.each do |shift| %>
      <tr>
        <td><%= shift.id %></td>
      <tr>
    <% end %>
  </table>
</div>

<div class="right-align" style="margin-right:30px; margin-bottom:30px">
  <div className="fixed-action-btn">
    <button class="btn-floating btn-large red">
        <i class="material-icons icon-white"><%= link_to "add", new_team_shift_path, method: :get, class: "icon-white" %></i>
    </button>
  </div>
</div>
